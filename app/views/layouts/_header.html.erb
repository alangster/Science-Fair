<header>
  <nav>
    <ul>
      <% if !current_user %>
      <li id="signin_link"><%= link_to "Login", new_session_path %></li>
      <li id="signup_link"><%= link_to "Sign Up", new_user_path %></li>
      <% else %>
      <li id="logout_link"><%=  link_to "Logout", session, method: :delete %></li>
      <li id="profile_link"><%= link_to "Profile", current_user %></li>
      <% end %>
    </ul>
  </nav>
</header>
